    <contexts>
      <context name="Normal" attribute="Normal" lineEndContext="#stay">
        <Detect2Chars char="/" char1="/" attribute="Comment" context="LineComment"/>
        <Detect2Chars char="/" char1="*" attribute="Comment" context="BlockComment"/>
        <DetectChar char="#" attribute="Preprocessor" context="Preprocessor"/>
        <DetectChar char="&quot;" attribute="String" context="DoubleQuotedString"/>
        <DetectChar char="'" attribute="String" context="SingleQuotedString"/>
        <Float attribute="Float" context="#stay"/>
        <Int attribute="Decimal" context="#stay"/>
        <RegExpr String="\.(\w+)" lookAhead="true" context="DotProperty"/>
        <AnyChar attribute="Punctuation" String="(),.:;[]{}" context="#stay"/>
        <RegExpr String="(?:!=|&amp;&amp;|&lt;[&amp;&lt;=]|==|>[=>]|\|\||[!%&amp;*+/&amp;&lt;>|^-])" attribute="Operator" context="#stay"/>
        <keyword String="types" attribute="Type" context="FunctionMaybe"/>
        <WordDetect String="function" attribute="Keyword" context="FunctionMaybe"/>
        <WordDetect String="enum" attribute="Keyword" context="Enum"/>
        <WordDetect String="struct" attribute="Keyword" context="Struct"/>
        <keyword String="controlflow" attribute="ControlFlow" context="#stay"/>
        <keyword String="keywords" attribute="Keyword" context="#stay"/>
        <keyword String="booleans" attribute="Boolean" context="#stay"/>
        <keyword String="enumtypes" attribute="EnumType" context="#stay"/>
        <keyword String="enumvalues" attribute="EnumValue" context="#stay"/>
        <RegExpr String="eMode\w+" attribute="EnumValue" context="#stay"/>
        <RegExpr String="eFont\w+" attribute="EnumValue" context="#stay"/>
        <keyword String="dialogtokens" attribute="Constant" context="#stay"/>
        <keyword String="globalfunctions" attribute="GlobalFunction" context="#stay"/>
        <keyword String="globalobjects" attribute="GlobalObject" context="#stay"/>
        <keyword String="globalarrays" attribute="GlobalArray" context="#stay"/>
        <keyword String="globalstates" attribute="GlobalState" context="#stay"/>
        <WordDetect String="Point" attribute="ManagedType" context="#stay"/>
        <StringDetect String="AGS_MAX_HOTSPOTS" attribute="Constant" context="#stay"/>
        <StringDetect String="AGS_MAX_INV_ITEMS" attribute="Constant" context="#stay"/>
        <StringDetect String="AGS_MAX_OBJECTS" attribute="Constant" context="#stay"/>
        <StringDetect String="AGS_MAX_REGIONS" attribute="Constant" context="#stay"/>
        <StringDetect String="AGS_NEW_STRINGS" attribute="Constant" context="#stay"/>
        <StringDetect String="AGS_SUPPORTS_IFVER" attribute="Constant" context="#stay"/>
        <StringDetect String="DEBUG" attribute="Constant" context="#stay"/>
        <StringDetect String="LRPRECEDENCE" attribute="Constant" context="#stay"/>
        <StringDetect String="MAX_LISTBOX_SAVED_GAMES" attribute="Constant" context="#stay"/>
        <StringDetect String="NEW_DIALOGOPTS_API" attribute="Constant" context="#stay"/>
        <StringDetect String="NEW_KEYINPUT_API" attribute="Constant" context="#stay"/>
        <StringDetect String="PALETTE_SIZE" attribute="Constant" context="#stay"/>
        <RegExpr String="SCRIPT_API_v\d+" attribute="Constant" context="#stay"/>
        <RegExpr String="SCRIPT_COMPAT_v\d+" attribute="Constant" context="#stay"/>
        <StringDetect String="STRICT_AUDIO" attribute="Constant" context="#stay"/>
        <StringDetect String="STRICT_STRINGS" attribute="Constant" context="#stay"/>
        <StringDetect String="STRICT" attribute="Constant" context="#stay"/>
      </context>

      <context name="Preprocessor" attribute="Preprocessor" lineEndContext="#pop">
      </context>

      <context name="DotProperty" lineEndContext="#stay">
        <DetectChar char="." attribute="Punctuation" context="#stay"/>
        <StringDetect String="%1" dynamic="true" attribute="Property" context="#pop"/>
      </context>

      <context name="FunctionMaybe" fallthroughContext="#pop" lineEndContext="#stay">
        <Detect2Chars char="[" char1="]" attribute="Punctuation" context="#stay"/>
        <DetectSpaces/>
        <WordDetect String="noloopcheck" attribute="Keyword" context="#stay"/>
        <DetectSpaces/>
        <RegExpr String="(\w+(?:::\w+)?)\s*\(" lookAhead="true" context="#pop!FunctionName"/>
      </context>

      <context name="FunctionName" fallthroughContext="#pop" lineEndContext="#pop">
        <StringDetect String="%1" dynamic="true" attribute="Function" context="#pop"/>
      </context>

      <context name="Enum" fallthroughContext="#pop" lineEndContext="#stay">
        <DetectSpaces/>
        <RegExpr String="\w+" attribute="Enum" context="#pop"/>
      </context>

      <context name="Struct" fallthroughContext="#pop" lineEndContext="#stay">
        <DetectSpaces/>
        <RegExpr String="\w+" attribute="Struct" context="#pop"/>
      </context>

      <context name="SpecialInString">
        <DetectChar char="[" attribute="NewLine" context="#stay"/>
        <RegExpr String="\\." attribute="EscapeSequence" context="#stay"/>
      </context>

      <context name="DoubleQuotedString" attribute="String" lineEndContext="#stay">
        <IncludeRules context="SpecialInString"/>
        <DetectChar char="&quot;" attribute="String" context="#pop"/>
      </context>

      <context name="SingleQuotedString" attribute="String" lineEndContext="#stay">
        <IncludeRules context="SpecialInString"/>
        <DetectChar char="'" attribute="String" context="#pop"/>
      </context>

      <context name="LineComment" attribute="Comment" lineEndContext="#pop">
